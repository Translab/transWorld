  version: 1
  code: "float size = $spotSize + (sin(_Time.x * 10+p.y) + sin(p.x*3) * 0.1 + sin(p.z*3)
    * 0.1) * 0.05;\nfloat separation = 2.0;\nfloat4 settings = float4($spotSpacing,
    2.0, 2.0, 1.0);\nfloat3 pi = float3(0,-0.5,0) - p;\nfloat s = sphere(\n  opRep(\n
    \   pi * (1.0/separation * 10.0),\n    float3(settings.x, settings.y, settings.z)),\n
    \   size * max(1.0, settings.w)\n);\nfloat v = s < 0.0025 ? 0.5 : 1.0;\n\nreturn
    lerp(1, v, 0.5) * $color.rgb;"
