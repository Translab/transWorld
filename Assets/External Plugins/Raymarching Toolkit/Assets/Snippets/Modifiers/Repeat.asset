  m_Script: {fileID: 913257115, guid: 1741f56c22154746ae9dc695200288e6, type: 3}
  m_Name: Repeat
  m_EditorClassIdentifier: 
  version: 1
  code: 'float3 repeat = $separation;

    float3 op = $separation * .5 + p;

    float3 cell = floor(op / $separation);

    float3 result = sign(op / repeat) * (op % repeat) - 0.5 * repeat;

    result = lerp(p, result, float3($x, $y, $z));


    // noise

    $noisedisplacement = clamp($noisedisplacement, 0, $separation * .5);

    float np = ((cell.z + cell.x + cell.y) / 100) * 2;

    float n = 0.5 + (tex2Dlod($noisetex, float4(np % 1, 0, 0, 0)).r - 0.5) * 2;

    //n = cnoise(float2(np,0));

    // translate

    result += n * $noisedisplacement;


    return result;'
